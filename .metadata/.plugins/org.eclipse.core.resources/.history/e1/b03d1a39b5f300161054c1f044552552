package feb15_2017;

public class LoopDeDooooo22 {

	public static void main(String[] args) {
		int num = 0;
		
		while(num <= 10000) {
			if (isPrime(num)) {
				System.out.println(num);
			}
			num++;
		}
	}
	
	public static boolean isPrime(int n) {
		
		if (n % 2 == 0 || n % 3 == 0 || n % 5 == 0 || n % 7 == 0 || n % 9 == 0) {
			return false;
		}
		else {
			return true;
		}
		
	}
}
